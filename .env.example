# Face Finder Configuration
# Copy this file to .env and update values as needed

# API Settings
API_TITLE=Face Finder API
API_DESCRIPTION=Face detection, embedding, and search using SCRFD, LVFace, and Qdrant
API_VERSION=1.0.0
API_HOST=0.0.0.0
API_PORT=8000

# Face Detection Settings (SCRFD)
SCRFD_MODEL_PATH=models/scrfd.onnx
DETECTION_THRESHOLD=0.5

# Face Embedding Settings (LVFace)
LVFACE_MODEL_PATH=models/lvface.onnx
USE_GPU=true
EMBEDDING_DIM=512

# Qdrant Settings
# Option 1: Local Qdrant (Docker)
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Option 2: Qdrant Cloud (uncomment and set these, takes precedence over host/port)
# QDRANT_URL=https://your-cluster.region.cloud.qdrant.io:6333
# QDRANT_API_KEY=your_api_key_here

QDRANT_COLLECTION_NAME=face_embeddings

# Search Settings
SIMILARITY_THRESHOLD=0.6
MAX_SEARCH_RESULTS=10
